              <div class="mg-bot-date">
                <%= @tour.start_date %> &nbsp;&nbsp;&nbsp;&nbsp;
                <span class="hidden-xs">
                  <i class="far fa-calendar-alt"></i>&nbsp;
                  <a href="/danh-sach-tour/NDSGN535/da-lat-may-bay-kim-ngan-hill-phim-truong-happy-hill-que-garden-tour-gia-soc.aspx" class="b">Ngày khác</a>
                </span>
              </div>
              <div class="hidden-lg hidden-md hidden-sm">
                <i class="far fa-calendar-alt"></i>&nbsp;<a href="/danh-sach-tour/NDSGN535/da-lat-may-bay-kim-ngan-hill-phim-truong-happy-hill-que-garden-tour-gia-soc.aspx" class="b">Ngày khác</a>
              </div>
            </div>
          </div>
