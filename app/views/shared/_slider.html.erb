<div class="n3-slideshow">
  <div id="myCarousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators hidden">
      <li data-target="#myCarousel1" data-slide-to="0" class=""></li>
      <li data-target="#myCarousel1" data-slide-to="1" class=""></li>
      <li data-target="#myCarousel1" data-slide-to="2" class=""></li>
      <li data-target="#myCarousel1" data-slide-to="3" class=""></li>
      <li data-target="#myCarousel1" data-slide-to="4" class="active"></li>
      <li data-target="#myCarousel1" data-slide-to="5" class=""></li>
    </ol>
    <div class="carousel-inner">
      <div class="item">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider1'%>
        <% end %>
      </div>
      <div class="item">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider2' %>
        <% end %>
      </div>
      <div class="item">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider3'%>
        <% end %>
      </div>
      <div class="item">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider4', class: "active" %>
        <% end %>
      </div>
      <div class="item active">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider5'%>
        <% end %>
      </div>
      <div class="item">
        <%= link_to '#' do %>
          <%= image_tag 'travel-slider6'%>
        <% end %>
      </div>
    </div>
    <a class="left carousel-control hidden-xs" href="#myCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control hidden-xs" href="#myCarousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
