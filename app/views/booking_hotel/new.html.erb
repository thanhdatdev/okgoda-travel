  function formatnumber(what) {
    if (what != null) {
      var value = what;
      value = value.replace(',', '');
      value = value.replace(',', '');
      value = value.replace(',', '');
      var temp = '';
      var test = value.length;
      var count = 0;
      for (i = test - 1; i >= 0; i--) {
        count++
        temp = value.charAt(i) + temp;
        if (count % 3 == 0 && i > 0) {
          temp = ',' + temp;
        }
      }
      return temp;
    }
    return "";
  }
