            <div class="col-lg-4 col-md-2 col-sm-3 col-xs-6" style="padding-right: 0px !important;">Nơi khởi hành:</div>
            <div class="col-lg-8 col-md-10 col-sm-9 col-xs-6">
              <%= @tour.departure %>
            </div>
          </div>
        </div>
        <div class="sec3">
          <div class="row">
